commands:
  01_export_env_vars:
    command: 'export DATABASE_URL=postgresql://$RDS_USERNAME:$RDS_PASSWORD@$RDS_HOSTNAME:$RDS_PORT/$RDS_DB_NAME'
    ignoreErrors: false
  02_prisma_generate:
    command: 'npx prisma generate'
    ignoreErrors: false
  03_prisma_migrate:
    command: 'npx prisma migrate deploy'
    ignoreErrors: false
